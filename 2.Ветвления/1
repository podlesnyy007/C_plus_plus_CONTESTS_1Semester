#include <iostream>
using namespace std;
#include <cmath>

int main()
{
	double x, y;
	cin >> x >> y;
	double a = sqrt(pow(x, 2) + pow(y, 2));
	if (a <= 2 && not(0 <= x && y >= 0)) {
		cout << "Yes";
	}
	else {
		cout << "No";
	}
}

#include <iostream>
using namespace std;

int main()
{
	double x, y;
	cin >> x >> y;
	if (-2 <= x <= 2 && 0 <= y <= -2 || 0 <= x <= 2 && 0 <= y <= 2) {
		cout << "Yes";
	}
	else {
		cout << "No";
	}
}
