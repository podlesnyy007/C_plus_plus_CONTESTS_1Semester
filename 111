#include <iostream>
using namespace std;
#include <cmath>
#include <iomanip>

int tp(int x, int y) {
    return sqrt(pow(x, 2) + pow(y, 2));
}

int main()
{
    float a, b, c, d, e;
    cin >> a >> b >> c;
    d = tp(a, b);
    e = tp(c, d);
    cout << fixed << setprecision(2) << e;
    return 0;
}
